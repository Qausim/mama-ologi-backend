{
  "name": "mama-ologi-backend",
  "version": "1.0.0",
  "description": "The backend for an e-commerce application for the Mama Ologi (Papseller) brand",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src --fix",
    "start": "node build/",
    "build": "npm run clean && babel src -d build src/ -s",
    "pretest": "NODE_ENV=test npm run undo:migration && npm run migration && npm run seed",
    "test": "NODE_ENV=test nyc mocha -r @babel/polyfill -r @babel/core -r @babel/register --recursive src/tests --timeout 3000 --exit",
    "posttest": "NODE_ENV=test npm run undo:migration",
    "serve": "babel-node src/",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test && nyc report --reporter=text-lcov | coveralls",
    "start:dev": "nodemon --exec babel-node src/index.js",
    "clean": "rimraf ./build",
    "migration": "babel-node src/db/migration.js",
    "seed": "babel-node src/tests/mock",
    "undo:migration": "babel-node src/db/undoMigration.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Qausim/mama_ologi_backend.git"
  },
  "keywords": [
    "Pap",
    "Ogi",
    "Breakfast",
    "food",
    "baby food",
    "nursing mother",
    "cereal"
  ],
  "author": "Olawumi Qauzeem Yusuff",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Qausim/mama_ologi_backend/issues"
  },
  "homepage": "https://github.com/Qausim/mama_ologi_backend#readme",
  "dependencies": {
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.21.0",
    "connect-multiparty": "^2.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^7.18.2",
    "sinon": "^7.5.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.9.2",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^6.2.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.2"
  }
}
