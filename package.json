{
  "name": "mama-ologi-backend",
  "version": "1.0.0",
  "description": "The backend for an e-commerce application for the Mama Ologi (Papseller) brand",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src --fix",
    "start": "node build/",
    "build": "npm run clean && babel src -d build src/ -s",
    "test": "NODE_ENV=test npm run migration && nyc mocha -r @babel/polyfill -r @babel/core -r @babel/register --recursive src/tests --timeout 3000 --exit && npm run undo:migration",
    "serve": "babel-node src/",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test && nyc report --reporter=text-lcov | coveralls",
    "start:dev": "nodemon --exec babel-node src/index.js",
    "clean": "rimraf ./build",
    "migration": "babel-node src/db/migration.js",
    "undo:migration": "babel-node src/db/undoMigration.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Qausim/mama_ologi_backend.git"
  },
  "keywords": [
    "Pap",
    "Ogi",
    "Breakfast",
    "food",
    "baby food",
    "nursing mother",
    "cereal"
  ],
  "author": "Olawumi Qauzeem Yusuff",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Qausim/mama_ologi_backend/issues"
  },
  "homepage": "https://github.com/Qausim/mama_ologi_backend#readme",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.15.0",
    "connect-multiparty": "^2.2.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^7.14.0",
    "sinon": "^7.5.0",
    "swagger-ui-express": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.3",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0"
  }
}
